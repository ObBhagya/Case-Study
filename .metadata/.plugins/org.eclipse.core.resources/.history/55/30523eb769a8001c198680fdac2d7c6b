package Practise;

import java.util.ArrayList;
import java.util.Collections;
import java.util.List;

public class Compratr_Aerylst_Sort {

	public static void main(String[] args) {
		
		ArrayList<Emply> arrl= new ArrayList<Emply>();
		arrl.add(new Emply(1, "aa", 33));
		arrl.add(new Emply(2, "bb", 23));
		arrl.add(new Emply(3, "cc", 44));
		
		Collections.sort(arrl.new List());
	
		
		
	}

}
