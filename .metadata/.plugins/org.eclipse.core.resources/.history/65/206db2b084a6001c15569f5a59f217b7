package Logical_Program;

public class Icr_Dcr {

	public static void main(String[] args) {
		
      int a=10;
      int b=20;
      
      int incr=(a++) + (++b);
      //a=10 b=21 
      
      System.out.println("a-"+a);
      
      System.out.println("b-"+b);
      
      
      System.out.println("incr"+incr);
      
		
		
	}

}
