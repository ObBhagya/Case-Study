package com.onebill.asgn1;

public class c3 {
	public int check(int x, int y) {
		
		int sum=x+y;
		
		if (x==0 && y==0) {
			System.out.println("invalid value");
		}
		
		if(x!=y) {
			System.out.println("both numberes are not equal");
			
			
		}
		
		if(x==y) {
			System.out.println("numbers are equal");
		}
		
		return sum;
		
		
		
		
	}

}
